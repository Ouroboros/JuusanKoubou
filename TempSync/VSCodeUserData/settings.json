// Place your settings in this file to overwrite the default settings
{
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "workbench.colorCustomizations": {
        "tab.inactiveForeground": "#8A8A8A",
        "editorLineNumber.foreground": "#828282"
    },

    "editor.cursorBlinking": "phase",
    "editor.smoothScrolling": true,
    "editor.fontFamily": "Fira Code Retina",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "editor.renderWhitespace": "all",
    "editor.minimap.showSlider": "always",
    "files.trimTrailingWhitespace": true,
    "editor.multiCursorModifier": "alt",
    "editor.tokenColorCustomizations": {
        "comments": "#8A8A8A"
    },

    "breadcrumbs.enabled": true,
    "window.titleBarStyle": "custom",

    "window.newWindowDimensions": "inherit",
    "window.zoomLevel": 0,

    "files.associations": {
        "*.xm": "objective-cpp"
    },

    "python.pythonPath": "D:/Dev/Python/python.exe",
    "python.linting.enabled": false,
    "python.linting.pylintEnabled": false,
    "python.linting.pep8Enabled": false,
    "python.linting.flake8Enabled": false,
    "python.linting.lintOnSave": false,
    "python.linting.maxNumberOfProblems": 100,
    "python.linting.pylintCategorySeverity.convention": "Hint",
    "python.linting.pylintCategorySeverity.refactor": "Hint",
    "python.linting.pylintCategorySeverity.warning": "Warning",
    "python.linting.pylintCategorySeverity.error": "Error",
    "python.linting.pylintCategorySeverity.fatal": "Error",
    "python.linting.pylintPath": "pylint",
    "python.linting.pep8Path": "pep8",
    "python.linting.flake8Path": "flake8",
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Path": "autopep8",
    "python.formatting.yapfPath": "yapf",
    "python.autoComplete.extraPaths": [],
    "python.unitTest.nosetestsEnabled": false,
    "python.unitTest.nosetestPath": "nosetests",
    "python.unitTest.unittestEnabled": false,

    "typescript.npm": "D:/Dev/nodejs",

    "git.enabled": false,
    "git.autofetch": false,
    "git.autorefresh": false,
    "git.path": "D:/Dev/PortableGit/bin/git.exe",
    "git.ignoreMissingGitWarning": true,
    "explorer.confirmDelete": false,
    "files.eol": "\n",
    "extensions.ignoreRecommendations": false
}